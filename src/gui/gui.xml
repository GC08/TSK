<?xml version="1.0" encoding="UTF-8"?>
 
<?import javafx.scene.text.*?>
<?import javafx.scene.shape.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.Circle?>

<AnchorPane fx:id="guiContent"
	xmlns="http://javafx.com/javafx/8.0.40" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="gui.GuiController">
	<VBox>
		<Button fx:id="goToDiode" 
			onAction="#diodeButtonAction" prefHeight="28.0" prefWidth="500.0"
			text="Diody" textFill="#115b0c">
			<font>
				<Font name="DejaVu Serif Bold" size="14.0" />
			</font>
		</Button>
		
		<Button fx:id="goToRCFilters" 
			onAction="#rcButtonAction" prefHeight="28.0" prefWidth="500.0"
			text="Filtry RC" textFill="#115b0c">
			<font>
				<Font name="DejaVu Serif Bold" size="14.0" />
			</font>
		</Button>
		
		<Button fx:id="goToRLFilters" 
			onAction="#rlButtonAction" prefHeight="28.0" prefWidth="500.0"
			text="Filtry RL" textFill="#115b0c">
			<font>
				<Font name="DejaVu Serif Bold" size="14.0" />
			</font>
		</Button>
		
		<Button fx:id="goToRLC" 
			onAction="#rlcButtonAction" prefHeight="28.0" prefWidth="500.0"
			text="Równoległy układ RLC(filtr pasmowoprzepustowy)" textFill="#115b0c">
			<font>
				<Font name="DejaVu Serif Bold" size="14.0" />
			</font>
		</Button>
	</VBox>
</AnchorPane>